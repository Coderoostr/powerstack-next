{
  "name": "powerstack-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "gen-graphql": "env-cmd -f .env graphql-codegen --config ./app-engine/graphql/codegen.yml && eslint --fix ./app-engine/graphql/generated/**/*.ts",
    "prisma-gen": "prisma db pull && prisma generate",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "theme-gen": "node ./_scripts/theme-gen.js",
    "lhci:mobile": "lhci autorun",
    "lhci:desktop": "lhci autorun --collect.settings.preset=desktop"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@cloudinary/url-gen": "^1.8.0",
    "@sentry/browser": "^7.8.0",
    "@stitches/react": "^1.2.8",
    "@web3auth/base": "^1.1.2",
    "@web3auth/web3auth": "^1.1.2",
    "anchor-link": "^3.5.1",
    "anchor-link-browser-transport": "^3.5.1",
    "apollo-link-sentry": "^3.2.0",
    "auto-zustand-selectors-hook": "^2.0.0",
    "env-var": "^7.1.1",
    "ethers": "^5.6.9",
    "graphql": "^16.5.0",
    "graphql-ws": "^5.9.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "next": "12.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "simple-zustand-devtools": "^1.1.0",
    "zustand": "^4.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.6.1",
    "@types/react": "^18.0.15",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.3",
    "typescript": "^4.7.4"
  }
}
